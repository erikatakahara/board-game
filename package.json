{
  "name": "board-game",
  "description": "Demo of how to build an app using marko-starter",
  "version": "1.0.0",
  "dependencies": {
    "marko": "^4.15.2",
    "marko-starter": "^2.0.4",
    "prompt": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "marko-prettyprint": "^2.0.0",
    "prettier": "^1.16.4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marko-js-samples/marko-starter-demo"
  },
  "scripts": {
    "build": "NODE_ENV=production marko-starter build",
    "format": "prettier src/**/*.{js,css,less} --write && marko-prettyprint src",
    "lint": "eslint src",
    "serve": "NODE_ENV=production marko-starter serve-static",
    "start": "marko-starter server",
    "publish": "LASSO_TIMEOUT=60000 NODE_ENV=production node ./publish.js",
    "test": "npm run lint"
  },
  "private": true,
  "static-repo": "git@github.com:erikatakahara/board-game.git#gh-pages",
  "baseurl": "/board-game"
}
